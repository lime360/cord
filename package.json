{
  "name": "cord",
  "description": "Discord web client wrapper with disabled telemetry and custom theme support",
  "version": "0.0.1",
  "type": "module",
  "main": "app.mjs",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-dmg": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "electron": "^37.2.4"
  },
  "scripts": {
    "start": "electron-forge start",
    "make": "electron-forge make --targets=@electron-forge/maker-zip",
    "make-windows": "electron-forge make --targets=@electron-forge/maker-squirrel",
    "make-mac": "electron-forge make --targets=@electron-forge/maker-dmg"
  }
}
